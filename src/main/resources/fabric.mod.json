{
  "schemaVersion": 1,
  "id": "rocket-crafter",
  "version": "${version}",
  "name": "Addon Template",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "seasnail"
  ],
  "contact": {
    "repo": "https://github.com/MeteorDevelopment/meteor-rocket-crafter"
  },
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "der.udo.rocketcrafter.RocketCraft"
    ]
  },
  "accessWidener": "rocket-crafter.accesswidener",
  "mixins": [
    "rocket-crafter.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "225,25,25"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
